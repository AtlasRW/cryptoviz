{
  "name": "pilea",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "bundle": "./bin/webpack-dev-server",
    "bundle:clean": "rimraf public/packs public/packs-test",
    "bundle:prod": "NODE_ENV=production ./bin/webpack --progress",
    "server": "RAILS_ENV=development rails server",
    "server:prod": "RAILS_ENV=production RAILS_SERVE_STATIC_FILES=enabled rails server",
    "start": "yarn bundle:clean && yarn bundle:prod && yarn server",
    "start:prod": "yarn bundle:clean && yarn bundle:prod && yarn server:prod"
  },
  "dependencies": {
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "4.2.2",
    "mjml": "^4.6.3",
    "tailwindcss": "^1.4.6",
    "turbolinks": "^5.2.0",
    "typeface-inter": "^3.12.0",
    "typeface-leckerli-one": "^0.0.72",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "webpack-dev-server": "^3.11.0"
  }
}
